/* pages/home/home.wxss */
.swiperbox {
  width: 90%;
  margin: 0 auto;
  box-shadow: 0 3rpx 3rpx 0 #eee;
}
.swiper {
  height: 300rpx;
  border-radius: 20rpx;
  overflow: hidden;
}

.contain {
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  background-color: #fff;

  }
.contain view {
  text-align: center;
  flex: 30%;
}
.contain image {
  display: block;
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  padding-bottom: 10rpx;
}
.contain text {
  color: #3d3d3d;
  font-size: 14px;
}